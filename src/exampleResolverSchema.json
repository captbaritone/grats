{
  "$schema": "./resolverSchema.json",
  "types": {
    "Query": {
      "someProp": {
        "kind": "property",
        "name": "someProp"
      },
      "hello": {
        "kind": "function",
        "path": "./resolvers/hello.js",
        "exportName": "hello",
        "arguments": [{ "kind": "named", "name": "someArg" }]
      },
      "greet": {
        "kind": "method",
        "arguments": [{ "kind": "named", "name": "someArg" }]
      },
      "goodbye": {
        "kind": "staticMethod",
        "path": "./resolvers/goodbye.js",
        "name": "goodbye",
        "arguments": [{ "kind": "named", "name": "someArg" }]
      }
    }
  }
}
