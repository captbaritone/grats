-- TypeScript --
import type TSomeType from "./IdField";
import { GraphQLSchema, GraphQLObjectType, GraphQLID } from "graphql";
export function getSchema(): GraphQLSchema {
    const SomeTypeType: GraphQLObjectType = new GraphQLObjectType<TSomeType>({
        name: "SomeType",
        fields() {
            return {
                id: {
                    name: "id",
                    type: GraphQLID
                }
            };
        }
    });
    return new GraphQLSchema({
        types: [SomeTypeType]
    });
}
