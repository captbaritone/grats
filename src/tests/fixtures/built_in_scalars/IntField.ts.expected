-- TypeScript --
import { GraphQLSchema, GraphQLObjectType, GraphQLInt } from "graphql";
export function getSchema(): GraphQLSchema {
    const SomeTypeType: GraphQLObjectType = new GraphQLObjectType({
        name: "SomeType",
        fields() {
            return {
                age: {
                    name: "age",
                    type: GraphQLInt
                }
            };
        }
    });
    return new GraphQLSchema({
        types: [SomeTypeType]
    });
}
