/** @gqlType */
export class SomeType {
  /** @gqlField */
  greeting(ctx: SomeOtherType, alsoContext: SomeOtherType): string {
    return ctx.greeting ?? "Hello, world!";
  }
}

/** @gqlContext */
type SomeOtherType = { greeting?: string };

