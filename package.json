    "semver": "^7.7.2",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jest-diff": "^30.1.2",
    "node-fetch": "^3.3.2",
    "path-browserify": "^1.0.1",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "ts-node": "^10.9.2"
